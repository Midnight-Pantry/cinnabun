{
  "compilerOptions": {
    "rootDir": ".",
    "declaration": true,
    //"emitDeclarationOnly": true,
    "outDir": "./dist",
    "allowJs": true,
    "checkJs": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "nodenext"
