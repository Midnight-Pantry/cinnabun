{
  "name": "cinnabun",
  "version": "0.1.18",
  "description": "A reactive, component-based web framework built with Typescript & esbuild",
  "repository": {
    "type": "git",
    "url": "https://github.com/Robby6Strings/cinnabun"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./ssr": "./dist/ssr/index.js",
    "./router": "./dist/router/index.js",
    "./utils": "./dist/utils.js",
    "./domInterop": "./dist/domInterop.js",
    "./hydration": "./dist/hydration.js",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "build": "pnpm i && tsc",
    "mocha": "mocha ./src/tests/*/**.spec.js",
    "cypress": "npx cypress run --spec ./cypress/e2e --env port=3000",
    "cypress-csr": "npx cypress run --spec ./cypress/e2e/csr --env port=5173",
    "cypress-ssr": "npx cypress run --spec ./cypress/e2e/ssr --env port=3000",
    "cypress-open": "npx cypress open",
    "test": "pnpm build && pnpm mocha && pnpm cypress"
  },
  "keywords": [],
  "author": "Robby6Strings",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/jsdom": "^21.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "chai": "^4.3.7",
    "cypress": "^12.11.0",
    "esbuild": "^0.17.18",
    "jsdom": "^21.1.2",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  }
