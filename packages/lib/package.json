    "@types/node": "^18.15.11",
    "chai": "^4.3.7",
    "cypress": "^12.11.0",
    "esbuild": "^0.17.18",
    "jsdom": "^21.1.1",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.0.4"
  }
