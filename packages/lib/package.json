    "cypress-csr": "npx cypress run --spec ./cypress/e2e/csr --env port=5173",
    "cypress-ssr": "npx cypress run --spec ./cypress/e2e/ssr --env port=3000",
    "cypress-open": "npx cypress open",
    "test": "pnpm mocha && pnpm cypress"
  },
  "keywords": [],
